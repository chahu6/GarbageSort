.icon .coolc-list-cell.b-b:after {
  left: 90rpx;
}
.coolc-list-cell {
  display: flex;
  align-items: baseline;
  padding: 20rpx 30rpx;
  line-height: 60rpx;
  position: relative;
}
.coolc-list-cell.cell-hover {
  background: #fafafa;
}
.coolc-list-cell.b-b:after {
  left: 30rpx;
}
.coolc-list-cell .cell-icon {
  align-self: center;
  width: 56rpx;
  max-height: 60rpx;
  font-size: 46rpx;
}
.coolc-list-cell .cell-more {
  align-self: center;
  font-size: 30rpx;
  color: #606266;
  margin-left: 10rpx;
}
.coolc-list-cell .cell-tit {
  flex: 1;
  font-size: 28rpx;
  color: #303133;
  margin-right: 10rpx;
}
.coolc-list-cell .cell-tip {
  font-size: 26rpx;
  color: #909399;
}

